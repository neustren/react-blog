{
  "name": "bblog",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "startold": "webpack-dev-server --history-api-fallback --inline --hot",
    "start": "better-npm-run start-prod",
    "build": "webpack --verbose  --progress --colors --display-error-details --config webpack/prod.config.js",
    "dev": "concurrently --kill-others \"npm run start-dev\" \"npm run watch-client\"",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "development",
        "PORT": 3100,
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./app",
        "PORT": 3100,
        "APIPORT": 3030
      }
    },
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
