    "better-npm-run": "0.0.8",
    "bootstrap": "^3.3.6",
    "compression": "^1.6.1",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "helmet": "^1.3.0",
    "history": "^2.1.0",
    "html-to-react": "^1.0.0",
    "jquery": "^2.2.4",
